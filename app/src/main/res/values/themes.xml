<resources>
    <!-- Base application theme. -->
    <style name="Theme.GetAWay" parent="Theme.MaterialComponents.DayNight.NoActionBar.Bridge">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/yellow</item>
        <item name="colorPrimaryVariant">@color/charcoal</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <!--    DashBoard Theme-->
    <style name="ThemeOverlay.AppCompat.dashTheme" parent="ThemeOverlay.AppCompat">
        <item name="android:fontFamily">@font/allerta_stencil</item>
        <item name="android:drawableTint">@color/charcoal</item>
        <item name="colorPrimary">@color/yellow</item>
        <item name="background">@color/light_white</item>
        <item name="android:textColor">@color/charcoal</item>
    </style>

<!--    DashBoard Navigation Theme-->
    <style name="ThemeOverlay.AppCompat.dashNavTheme" parent="ThemeOverlay.AppCompat">
        <item name="android:fontFamily">@font/allerta_stencil</item>
        <item name="colorPrimary">@color/yellow</item>
        <item name="android:textColor">@color/charcoal</item>
        <item name="background">@color/light_white</item>
        <item name="colorControlHighlight">@color/charcoal</item>
    </style>


    //cursor Color--
    <style name="ThemeOverlay.AppTheme.TextInputEditText.Outlined" parent="">
        <item name="colorPrimary">@color/charcoal</item>
    </style>

</resources>